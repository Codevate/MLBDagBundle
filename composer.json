{
    "name": "marcobuschini/mlb-dag-bundle",
    "description": "This bundle provides entities, and methods to handle directed acyclic graphs.",
    "keywords": ["DAG", "graph", "acyclic"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco L. Buschini",
            "email": "marco.buschini@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.1",
        "symfony/symfony": "~2.3.41 || ~2.7.13 || ~2.8.6 || ~3.0.6"
    },
    "require-dev": {
        "doctrine/orm": "~2.3 || ~2.5",
        "doctrine/data-fixtures": "~1.0",
        "doctrine/doctrine-bundle": "~1.0",
        "phpunit/phpunit": "~4.1",
        "phpunit/php-code-coverage": "~2.2",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "satooshi/php-coveralls": "~1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "MLB\\DagBundle\\": ""
        }
    },
    "target-dir": "MLB/DagBundle"
}
